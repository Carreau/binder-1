{
  "name": "spark-master",
  "image": {{component.image-name}},
  "command": [
    "start-master.sh"
  ],
  "ports": [
    {
      "name": "master_port",
      "containerPort": {{service.master-port}}
    },
    {
      "name": "web_ui_port",
      "containerPort": {{service.web-ui-port}}
    }
  ],
  "resources": {
    "limits": {
      "cpu": "100m"
    }
  }
}
